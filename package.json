{
  "name": "scouting-app-backend",
  "version": "1.0.0",
  "description": "Backend for the Team 1334 Scouting App",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/migrations/**/*\" dist/migrations && copyfiles -u 1 \"config/**/*\" dist/config",
    "dev": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "sequelize-cli db:migrate"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.19.70",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^2.0.12",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "typescript": "^5.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "copyfiles": "^2.4.1",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.0",
    "@types/supertest": "^2.0.16",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
